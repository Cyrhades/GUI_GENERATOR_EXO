<HTA:APPLICATION
   ID="G²E"
   APPLICATIONNAME="GUI_GENERATOR_EXO"
   CAPTION="yes"
   BORDER=""
   ICON="favicon.ico"
   SHOWINTASKBAR="yes"
   SINGLEINSTANCE="no"
   SYSMENU="yes"
   WINDOWSTATE="yes"
   SCROLL="no"
   SCROLLFLAT="no"
   VERSION="1.0"
   INNERBORDER="yes"
   SELECTION="no"
   MAXIMIZEBUTTON="no"
   MINIMIZEBUTTON="no"
   NAVIGABLE="no"
   CONTEXTMENU="no"
   BORDERSTYLE="no"
   WIDTH="200px"
   HEIGHT="300px">
   <html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>GUI GENERATOR EXO</title>
        <style>
        body { margin :0; padding:0;}
        label.priority{margin-bottom:.3em;text-align:left; display:block;width:100%;}input.text{padding:.5em .6em; display:inline-block;border:1px solid grey;height:30px;width:300px;border-radius:4px;vertical-align:middle;box-sizing:border-box;}#step{width:50px;}#submit{background: #ff8d26;border: 5px solid #ff8d26;color:white;font-size:15pt;padding:8px 10px;margin:5px;white-space:normal;}
        #status_command{text-align :center; height: 70px; background-color: #ff8d26;}
       
        #menu { text-align:center; line-height:28px; height:30px; border-bottom : 3px solid #fdab28;}
        #menu a { width : 30%; text-decoration: none;  color: #2D31A9; font-weight: bold;}
        .tab {margin : 5px;}
        #menu a.selected {color: #fff; background-color: #ff8d26;}

        .icone{ width:48px; height:48px; }
        .icone_gulp{ background: url('data:image/png;base64,');}
        .icone_node{ background:url('data:image/png;base64,'); }
        .icone_npm{ background:url('data:image/png;base64,'); }
        .icone_yarn{ background:url('data:image/png;base64,'); }
        .icone_git{ background:url('data:image/png;base64,'); }
        .icone_php{ background: url('data:image/png;base64,')}
        .icone_symfony{ background:url('data:image/png;base64,'); }
        .icone_docker{ background:url('data:image/png;base64,'); }
        
        .logo-cmd{ width:48px; height:65px; margin : 0 10px 0 10px; border : 3px solid #ff8d26;}
        .logo-cmd.exists { border-bottom : 15px solid #fdab28;}
        .logo-cmd.notexists { filter: alpha(opacity=50); opacity: 0.50; border-bottom : 15px solid rgb(223, 48, 17);}       

        #tab_help { margin-top : 50px; background:#ff8d26; }
        #tab_help table tr td {font-weight: bold; padding-left: 5px; border : 3px solid #fdab28; color:#fff;}
        #tab_help table tr td a { color: #222; font-weight: bold;}
        #list_exos{ margin:3px;}
        .folder_exo {margin:3px; font-weight: bold;  padding: 5px; background:#fdab28; color:#fff; cursor:pointer; }
        .folder_exo.selected { background:#ff8d26; }
        </style>
    </head>
    <body>
        <div id="status_command"></div>
        <div id="menu"></div>
        <div id="tab_create_exercice" class="tab">
            <form>
                <p>
                    <label class="priority">Les différents énnoncés</label>
                    <input type="checkbox" id="doc_easy" checked="checked" disabled />
                    <label for="doc_easy">Facile</label>
                    
                    <input type="checkbox" id="doc_medium" checked="checked" disabled />
                    <label for="doc_medium">Intermediaire</label>

                    <input type="checkbox" id="doc_hard" checked="checked" disabled />
                    <label for="doc_hard">Difficile</label>
                </p>
                <p>
                    <label class="priority" for="name">Nom de l'exercice</label>
                    <input type="text" class="text" id="name" />
                </p>
                <p>
                    <label class="priority" for="dirname">Nom du répertoire</label>
                    <input type="text" class="text" id="dirname" /><small> (sera stocké dans ./src)</small>
                </p>
                <p>
                    <label class="priority" for="step">Nombre d'étape dans l'exercice</label>
                    <input type="text" class="text" id="step" size="2" maxlength="2" value="1" />
                </p>
                <p>
                    <label class="priority" for="git">Repository Git (source)</label>
                    <input type="text" class="text" id="git" style="width:460px" value="https://github.com/youpiwaza/boilerplate-exercice" />
                </p>

                <p style="text-align:center">
                    <button id="submit" onclick="createExo();">Générer l'exercice</button>
                </p>
            </form>
            <hr />
            <p style="text-align:center">
                <button id="submit" onclick="createDefaultExo();">Regénérer l'exercice par défaut</button>
            </p>        
        </div>

        <div id="tab_work_exercice" class="tab" style="display:none;">
            <p>
                <label for="list_exos">Liste des exercices</label>
                <div id="list_exos"></div>
            </p>
            <p>
                <input type="checkbox" id="start_server_with_php" disabled />
                <label style="color:grey" for="start_server_with_php">Démarrer un serveur web avec PHP</label>
            </p>
            <p>
                <input type="checkbox" id="https" disabled />
                <label style="color:grey" for="https">activer le HTTPS</label>
            </p>
            <p>
                <input type="checkbox" id="mysql" disabled />
                <label style="color:grey" for="mysql">Démarrer un conteneur Mysql avec Docker</label>
            </p>
            <p style="text-align:center">
                <button id="submit" onclick="watchExo();">Voir l'exercice</button>
            </p>

            <hr>

            <p>
                <label class="priority" for="git_dest">Repository Git (destination)</label>
                <input type="text" class="text" id="git_dest" style="width:560px" value="" />
            </p>
            <p>
                <label class="priority" for="git_comment">Commentaire sur commit</label>
                <input type="text" class="text" id="git_comment" style="width:560px" value="Commit depuis l'interface graphique" />
            </p>
            <p id="branch_git">
                
            </p>
            <p style="text-align:center">
                <button id="submit" onclick="commitExo();">Commit & Push exercice</button>
            </p>
        </div>

        <div id="tab_help" class="tab" style="display:none;">
            <table>
                <tr>
                    <td><span class="icone icone_node"></span></td>
                    <td>Pour installer nodeJs : 
                        <a href="https://nodejs.org/dist/v10.16.3/node-v10.16.3-x64.msi">Télécharger</a>
                    </td>
                </tr>
                <tr>
                    <td><span class="icone icone_npm"></span></td>
                    <td>NPM sera installé avec nodeJS</td>
                </tr>
                <tr>
                    <td><span class="icone icone_gulp"></span></td>
                    <td>
                        Installer Gulp
                        <a href="javascript:install('npm install gulp-cli -g')">Installer gulp</a>
                    </td>
                </tr>
                <tr>
                    <td><span class="icone icone_yarn"></span></td>
                    <td>
                        Installer Yarn
                        <a href="https://yarnpkg.com/latest.msi">Télécharger</a>
                    </td>
                </tr>
                <tr>
                    <td><span class="icone icone_git"></span></td>
                    <td>
                        Installer Docker
                        <a href="https://git-scm.com/download/win">Télécharger</a>
                    </td>
                </tr>
                <tr>
                    <td><span class="icone icone_php"></span></td>
                    <td>
                       Vous devez installer PHP en CLI (le dossier PHP doit être déclarée dans la variable d'environnement "path")
                    </td>
                </tr>
                <tr>
                    <td><span class="icone icone_symfony"></span></td>
                    <td>
                        Installer Symfony
                        <a href="https://get.symfony.com/cli/setup.exe">Télécharger</a>
                    </td>
                </tr>
                <tr>
                    <td><span class="icone icone_docker"></span></td>
                    <td>
                        Installer Docker
                        <a href="https://download.docker.com/win/stable/Docker%20for%20Windows%20Installer.exe">Télécharger</a>
                    </td>
                </tr>
            </table>
        </div>

        <script language="VBScript">
            // https://vb.developpez.com/faq/vbs?page=Shell-wshShell
            Set WshShell =  CreateObject("Wscript.Shell")

            Function VerifyCmdExists(cmd, arg)
                    On Error Resume Next 'Ceci ignore toute les erreurs

                    Return = WshShell.Run("cmd /c cd ../ & "&cmd&" "&arg , 0, true)  

                    If Return = "" Then
                        VerifyCmdExists = false
                    Else
                        VerifyCmdExists = true
                    End If
            End Function

            Function List(directory, currentExo)
                Set fs = CreateObject("Scripting.FileSystemObject")
                If fs.FolderExists(directory) Then
                    Set subfolders = fs.GetFolder(directory).SubFolders
                    options = ""
                    for each objFolder in subfolders
                        If currentExo = objFolder.Name Then
                            options = options & "<span class='folder_exo selected' onclick='javascript:selectExo(this);'>" & objFolder.Name & "</span>"
                        Else
                            options = options & "<span class='folder_exo' onclick='javascript:selectExo(this);'>" & objFolder.Name & "</span>"
                        End If
                    next
                    List = options
                    Else
                    List = "Dossier '<strong>src/</strong>' inexistant !"
                End If
            End Function


            Function VBCreateExo()

                WshShell.Run "cmd /c cd src/ &  git clone "&Document.getElementByID("git").Value&" & move boilerplate-exercice "&Document.getElementByID("dirname").Value&" & gulp generate-exercise & cd "&Document.getElementByID("dirname").Value&" & npm install"
                
                ' En attendant la correction du make-exercice pour avoir plusieurs exercices
                ' WshShell.Run "cmd /c gulp make-exercise",4,true
            End Function

            Function VBWatchExo(currentExo)
                WshShell.Run "cmd /k cd src/"&currentExo&" & gulp watch",4,true
            End Function

            Function VBCommitExo(currentExo,gitDist,gitComment)
                WshShell.Run "cmd /k cd src/"&currentExo&" & git add . & git commit -m """&gitComment&""" & git remote rm origin & git remote add origin "&gitDist&" & git push -u origin master",4,true
            End Function

        </script>

        <script type="text/javascript">
            var currentExo;
             // Liste des Commandes qui doivent être disponible
            var installCmd = { 
                node : false, npm : false, yarn : false, php : false,
                git : false, gulp : false, symfony : false, docker : false
            };
            var valuesDefault = { docs : "['enonce-easy', 'enonce-medium', 'enonce-hard']", git_repo : 'https://github.com/youpiwaza/boilerplate-exercice', exo_name : 'TEST-table-multiplication', exo_dirname : 'boilerplate-exercice', source : './src/', step : 7};
            var defaultConfig = "// // Configuration file, may export several configurations\nconst configExoTest = {\n  // .md filenames from which to generate folders\n  // correction will be generated automatically with ExercisePath/docs/correction.md AND git branches 'correction'\n  docs: %docs%,\n  // Exercise git repository uri\n  repoUri: '%git_repo%',\n  // Export name\n  name: '%exo_name%',\n  // Folder name in src/ ; going to work on /src/ExercisePath/src/\n  path: '%exo_dirname%',\n  // Where to clone repo, needs './' prefix\n  src: '%source%',\n  // Number of steps in step-by-step correction, must match ExercisePath/docs AND git branches 'step-*'\n  // Last step will be generated with the correction\n  steps: %step%,\n};\n\n\nmodule.exports = {\n  configExoTest,\n};\n"
            var current_directory = window.location.pathname.substring(0, window.location.pathname.lastIndexOf('\\'));
            //var current_directory = G²E.commandLine.substring(0, G²E.commandLine.lastIndexOf('\\'));
            
            function init()
            {
                window.resizeTo(640, 740);
                window.moveTo(300,150);

                createMenu();
                createListExos();
                createStatusCommandBar();  
            }

            function createListExos() {
                document.getElementById('list_exos').innerHTML = List("./src/", currentExo);
            }

            function selectExo(exo) {
                currentExo = exo.innerHTML;
                createListExos();
            }

            function watchExo() {
                if(currentExo == null) {
                    alert("vous devez choisir un exercice !");
                    return false;
                }
                VBWatchExo(currentExo)
            }

            function commitExo() {
                if(currentExo == null) {
                    alert("vous devez choisir un exercice !");
                    return false;
                }
                if(document.getElementById('git_dest').value == "") {
                    alert("vous devez préciser l'url de votre git !");
                    return false;
                }
                VBCommitExo(currentExo, document.getElementById('git_dest').value, document.getElementById('git_comment').value)
            }

            function createStatusCommandBar() 
            {
                // On vérifie que toute les installation nécessaire sont là !
                if(VerifyCmdExists('node','-v')) installCmd.node = true;
                if(VerifyCmdExists('npm','-v')) installCmd.npm = true;
                if(VerifyCmdExists('yarn','-v')) installCmd.yarn = true;
                if(VerifyCmdExists('gulp','-v')) installCmd.gulp = true;
                if(VerifyCmdExists('git','--version')) installCmd.git = true;
                if(VerifyCmdExists('php','-v')) installCmd.php = true;
                if(VerifyCmdExists('symfony','-v')) installCmd.symfony = true;
                if(VerifyCmdExists('docker','-v')) installCmd.docker = true;

                document.getElementById('status_command').innerHTML = 
                    '<span class="icone icone_node logo-cmd '+(installCmd.node?'':'not')+'exists"></span>'+
                    '<span class="icone icone_npm logo-cmd '+(installCmd.npm?'':'not')+'exists"></span>'+
                    '<span class="icone icone_gulp logo-cmd '+(installCmd.gulp?'':'not')+'exists"></span>'+
                    '<span class="icone icone_yarn logo-cmd '+(installCmd.yarn?'':'not')+'exists"></span>'+
                    '<span class="icone icone_git logo-cmd '+(installCmd.git?'':'not')+'exists"></span>'+
                    '<span class="icone icone_php logo-cmd '+(installCmd.php?'':'not')+'exists"></span>'+
                    '<span class="icone icone_symfony logo-cmd '+(installCmd.symfony?'':'not')+'exists"></span>'+
                    '<span class="icone icone_docker logo-cmd '+(installCmd.docker?'':'not')+'exists"></span>';
            }
  
            function changeTab(nameTab) 
            {
                var tabs = ['tab_create_exercice', 'tab_work_exercice', 'tab_help'];
                for(var i = 0; i < tabs.length; i++) {
                    document.getElementById(tabs[i]).style.display = "none"; 
                    document.getElementById('btn_'+tabs[i]).className = "";
                }
                document.getElementById(nameTab).style.display = "block"; 
                document.getElementById('btn_'+nameTab).className = "selected";
                
                if(nameTab == 'tab_work_exercice') createListExos();
            }

            function createMenu() 
            {
                document.getElementById('menu').innerHTML = 
                    '<a href="#" id="btn_tab_create_exercice" onclick="javascript:changeTab(\'tab_create_exercice\');" class="selected">Créer un exercice</a>'+
                    '<a href="#" id="btn_tab_work_exercice" onclick="javascript:changeTab(\'tab_work_exercice\');">Travailler sur un exercice</a>'+
                    '<a href="#" id="btn_tab_help" onclick="javascript:changeTab(\'tab_help\');">Aide installation</a>'
                ;
            }

            function generateDefaultContentConfigFile() 
            {
                newDefaultConfig = defaultConfig;
                newDefaultConfig = newDefaultConfig.replace("%docs%", valuesDefault.docs);
                newDefaultConfig = newDefaultConfig.replace("%git_repo%", valuesDefault.git_repo);
                newDefaultConfig = newDefaultConfig.replace("%exo_name%", valuesDefault.exo_name);
                newDefaultConfig = newDefaultConfig.replace("%exo_dirname%", valuesDefault.exo_dirname);
                newDefaultConfig = newDefaultConfig.replace("%source%", valuesDefault.source);
                newDefaultConfig = newDefaultConfig.replace("%step%",  valuesDefault.step);
                
                return newDefaultConfig;
            }

            function generateContentConfigFile() 
            {
                newDefaultConfig = defaultConfig;
                newDefaultConfig = newDefaultConfig.replace("%docs%", valuesDefault.docs);
                newDefaultConfig = newDefaultConfig.replace("%git_repo%", document.getElementById('git').value);
                newDefaultConfig = newDefaultConfig.replace("%exo_name%", document.getElementById('name').value);
                newDefaultConfig = newDefaultConfig.replace("%exo_dirname%", document.getElementById('dirname').value);
                newDefaultConfig = newDefaultConfig.replace("%source%", valuesDefault.source);
                newDefaultConfig = newDefaultConfig.replace("%step%", document.getElementById('step').value);
                
                return newDefaultConfig;
            }

            function createExo() 
            {
                if(createConfig(generateContentConfigFile())) {
                    VBCreateExo();
                }
            }

            function createDefaultExo() 
            {
                if(createConfig(generateDefaultContentConfigFile(), 'default')) {
                    VBCreateExo();
                }                
            }

            function createConfig(content, type) 
            {
                if(type != 'default' && (document.getElementById('dirname').value == "" || document.getElementById('dirname').value == "") ) {
                    alert("Vous devez renseigner les champs \"nom de l'exercice\" et \"nom du répertoire\".");
                    return false;
                }
                var fs = new ActiveXObject("Scripting.FileSystemObject");
                var configFile;
                if(type != 'default' && fs.FolderExists(current_directory+"\\src\\"+document.getElementById('dirname').value)) {
                    alert('Attention le dossier "src/'+document.getElementById('dirname').value+'" existe déjà, vous devez soit le supprimer ou le renommer pour ne pas avoir de conflit avec l\'interface graphique.');
                    return false;
                } else {      
                    if( fs.FileExists(current_directory+"\\gulp\\exercise\\config.js") === false) {
                        fs.CreateTextFile(current_directory+"\\gulp\\exercise\\config.js", true);
                    }

                    configFile = fs.OpenTextFile(current_directory+"\\gulp\\exercise\\config.js", 2);
   
                    configFile.Write(content);
                    return true;
                }
            }
            
            init();
        </script>
    </body>
</html>